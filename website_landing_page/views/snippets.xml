<template id="s_lp_date_counter_small" name="Date Counter">
	<div class="clock-small well s_well clearfix" >
		<div class="clock-small">
		<h4 class="text-center ">Event will start in</h4>
			<div class="dials">
				<div class="clock-days">
 					<div class="inner">
	                    <div class="dial">
	                        <p class="val">0</p>
	                        <p class="type-days type-time">DAYS</p>
	                    </div><!-- /.text -->
                	</div><!-- /.inner -->
				</div>				
				<div class="clock-hours">
 					<div class="inner">
	                    <div class="dial">
	                        <p class="val">0</p>
	                        <p class="type-hours type-time">HRS</p>
	                    </div><!-- /.text -->
                	</div><!-- /.inner -->
				</div>				
				<div class="clock-minutes">
 					<div class="inner">
	                    <div class="dial">
	                        <p class="val">0</p>
	                        <p class="type-days type-time">MIN</p>
	                    </div><!-- /.text -->
                	</div><!-- /.inner -->
				</div>				
				<div class="clock-seconds">
 					<div class="inner">
	                    <div class="dial">
	                        <p class="val">0</p>
	                        <p class="type-days type-time">SEC</p>
	                    </div><!-- /.text -->
                	</div><!-- /.inner -->
				</div>
        </div>
	</div>
 
	</div>
</template>

